@Article{Raikow2004,
  author   = {Raikow, D. F. and Sarnelle, O. and Wilson, A. E. and Hamilton, S. K.},
  title    = {Dominance of the noxious cyanobacterium {Microcystis} aeruginosa in low-nutrient lakes is associated with exotic zebra mussels},
  journal  = {Limnology and Oceanography},
  year     = {2004},
  volume   = {49},
  pages    = {482--487},
  month    = mar,
  issn     = {0024-3590},
  abstract = {To examine the hypothesis that invasion by zebra mussels (Dreissena polymorpha) promotes phytoplankton dominance by the noxious cyanobacterium Microcystis aeruginosa, 61 Michigan lakes of varying nutrient levels that contain or lack zebra mussels were surveyed during late summer. After accounting for variation in total phosphorus (TP) concentrations, lakes with Dreissena had lower total phytoplankton biomass, as measured by chlorophyll a and algal cell biovolume. Phytoplankton biomass increased with TP in both sets of lakes, although the elevations of the relationship differed. The percentage of the total phytoplankton comprised by cyanobacteria increased with TP in lakes without Dreissena (R-2 = 0.21, P = 0.025) but not in lakes with Dreissena (P = 0.79). Surprisingly, there was a positive influence of Dreissena invasion on Microcystis dominance in lakes with TP {\textless} 25 mug L-1 (P = 0.0018) but not in lakes with TP {\textgreater} 25 mug L-1 (P = 0.86). The finding that Microcystis, a relatively grazing-resistant component of the phytoplankton, was favored by Dreissena in low- but not in high-nutrient lakes is somewhat counterintuitive, but predator-prey models make this prediction in certain cases when the cost for the prey of being consumption resistant is a low maximum population growth rate. This Dreissena-cyanobacteria interaction contradicts well-established patterns of increasing cyanobacteria with nutrient enrichment in north-temperate lakes and suggests that the monitoring and abatement of nutrient inputs to lakes may not be sufficient to predict and control cyanobacterial dominance of Dreissena-invaded lakes.},
  doi      = {DOI 10.4319/lo.2004.49.2.0482},
  file     = {Raikow-2004-Dominance of the noxious cyanobact:/home/numen/Sync/Zotero/storage/B9CG4YDS/Raikow et al_2004_Dominance of the noxious cyanobacterium Microcystis aeruginosa in low-nutrient.pdf:application/pdf},
  keywords = {blue-green-algae, phytoplankton, dreissena-polymorpha, erie, food webs, hudson river, loading reductions, new-york, saginaw bay, total phosphorus},
  language = {English},
}

