	file = {Fontanillo et al_2016_Synthesis of Highly Selective Submicromolar Microcystin-Based Inhibitors of.pdf:C\:\\Users\\John Conner\\lztcz-uhxwx\\Zotero\\storage\\UX23E85M\\Fontanillo et al_2016_Synthesis of Highly Selective Submicromolar Microcystin-Based Inhibitors of.pdf:application/pdf}
