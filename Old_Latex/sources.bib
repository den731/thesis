	keywords = {microcystin, biosynthesis, diversity, microcystis, cyanobacterium microcystis, population, qpcr, aeruginosa pcc7806, bloom, genus microcystis, lake-erie, mcyd gene, nontoxic strains, peptide, san francisco estuary, toxic strains},
