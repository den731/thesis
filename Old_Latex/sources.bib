	keywords = {microcystin, cyanobacteria, aeruginosa, microcystis, lake, cyanobacterium microcystis, real-time pcr, bloom, peptide, flanking regions, phycocyanin intergenic spacer, quantification, strains, synthetase gene, toxic genotypes},
	pages = {188--193},
	annote = {714xpTimes Cited:46Cited References Count:46},
