	pages = {13985--13989},
	annote = {Fontanillo, MiriamZemskov, IvanHafner, MaximilianUhrig, UlrikeSalvi, FrancescaSimon, BerndWittmann, ValentinKohn, MajaengGermanyAngew Chem Int Ed Engl. 2016 Nov 2;55(45):13985-13989. doi: 10.1002/anie.201606449. Epub 2016 Oct 10.},
