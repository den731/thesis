	keywords = {Bacterial Toxins/*analysis/metabolism, Chromatography, High Pressure Liquid/methods, Cyanobacteria/*metabolism, Fresh Water/*chemistry, Marine Toxins/*analysis/metabolism, Microcystins/*analysis/metabolism, Saxitoxin/analysis/metabolism, Spectrometry, Mass, Electrospray Ionization/*methods, Tandem Mass Spectrometry/*methods, Tropanes/analysis/metabolism, Uracil/analogs \& derivatives/analysis/metabolism, Water Pollutants, Chemical/*analysis/metabolism},
