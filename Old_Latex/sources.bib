	keywords = {growth, river, turbulence, phosphorus, water, microcystis aeruginosa, alkaline phosphatase activity, bloom-forming microcystis, china, cyanobacterium, lake taihu, phosphatase-activity, small-scale turbulence, uptake kinetics},
	pages = {9251--9258},
