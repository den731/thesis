@article{charlton_oxygen_1980,
	title = {Oxygen {Depletion} in {Lake} {Erie}: {Has} {There} {Been} {Any} {Change}?},
	volume = {37},
	issn = {0706-652X},
	shorttitle = {Oxygen {Depletion} in {Lake} {Erie}},
	url = {http://www.nrcresearchpress.com/doi/abs/10.1139/f80-007},
	doi = {10.1139/f80-007},
	abstract = {A new analysis of hypolimnetic oxygen in Central Lake Erie indicates that historic increases in the apparent depletion were not as great as formerly believed. The differences that did occur were mostly related to variations in hypolimnion thickness. Changes, if any, in the oxygen depletion rate due to eutrophication are as yet too small to be recognized. Present-day oxygen depletion rates, when corrected for the relatively high temperatures in Lake Erie, are within the range thought to be indicative of mesotrophy in small lakes. The general level of oxygen depletion observed in the Central Basin of Lake Erie is expected on the basis of morphology alone.Key words: Lake Erie, oxygen, hypolimnion, oxygen depletion, trend, Une nouvelle analyse de l'oxygène hypolimnétique dans le bassin central du lac Erié indique que les augmentations historiques de l'épuisement apparent n'étaient pas aussi prononcées qu'on l'avait cru. Les différences qui se sont en fait produites étaient liées surtout aux variations d'épaisseur de l'hypolimnion. Les changements, s'il en est, de taux d'épuisement d'oxygène par eutrophisation sont encore trop faibles pour être identifiables. Les taux actuels d'épuisement d'oxygène, lorsque corrigés pour tenir compte des températures relativement élevées du lac Erié, se situent dans la gamme que l'on considère indicatrice de mésotrophie dans des petits lacs. On peut s'attendre, d'après la seule morphologie du lac, au niveau général d'épuisement d'oxygène observé dans le bassin central du lac Erié.},
	number = {1},
	urldate = {2018-06-23},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Charlton, M. N.},
	month = jan,
	year = {1980},
	pages = {72--80},
	file = {NRC Research Press Snapshot:C\:\\Users\\John Conner\\lztcz-uhxwx\\Zotero\\storage\\5ZHINIWN\\f80-007.html:text/html}
}

